import pygame
import math


pygame.init() 

display_width = 640
display_height = 480
display = pygame.display.set_mode((display_width, display_height))
pygame.display.set_caption("Vectors") 



run = True
while run: 
    for event in pygame.event.get(): 
        if event.type == pygame.QUIT: 
            run = False
    display.fill((100,100,100))











    pygame.display.update()


pygame.quit()
